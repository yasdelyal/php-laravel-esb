{
    "name": "yasdelyal/php-laravel-esb",
    "version": "0.0.1",
    "description": "Enterprise System Bus",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": [
        "laravel", "esb"
    ],
    "authors": [
        {
            "name": "Maksim Mikhaylov",
            "email": "2618094@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/support": "^6|^7|^8",
        "illuminate/contracts": "^6|^7|^8",
        "illuminate/events": "^6|^7|^8",
        "illuminate/console": "^6|^7|^8",
        "illuminate/bus": "^6|^7|^8",
        "enqueue/amqp-ext": "^0.10.9"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "YaSdelyal\\Esb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YaSdelyal\\Esb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YaSdelyal\\Esb\\ServiceProvider"
            ]
        }
    }
}
